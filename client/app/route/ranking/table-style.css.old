table.type12 {
	width: 200px;
	border-collapse: separate;
	border-spacing: 3px;
	text-align: center;
        font-size: 20px;
	line-height: 1.5;
	vertical-align: top;
}
table.type12 th {
	padding: 0px;
        border-radius: 5px 5px 10px 10px;
}
table.type12 td {
	padding: 30px 10px;
	vertical-align: top;
	background: #eee;
	height: 50px;
	position:relative
	fontsize: 24px;
	table-hover;
}
table.type12 th.col01 {background: #060;}
table.type12 td.col01 {background: #cfc;}
table.type12 th.col02 {background: #f60;}
table.type12 td.col02 {background: #fc9;}
table.type12 th.col03 {background: #036;}
table.type12 td.col03 {background: #e2eeff;}